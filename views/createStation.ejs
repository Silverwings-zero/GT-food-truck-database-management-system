<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Create Station</title>
</head>
<body>
    <form action="/createStation" method="POST">
        <h1>Create Station</h1>
        <div>
            <label for="sname">Name: </label>
            <input type="text" name="sname" id="sname" required/>
        </div>
        <div>
            <label for="capacity">Capacity: </label>
            <input type="number" name="capacity" id="capacity" required>
        </div>
        <div>
            <label for="sb">Sponsored Building: </label>
            <select name="sb" id="sb" required>
                <%  var buildingName = JSON.parse(buildingName);
                    for (let i = 0; i < buildingName.length; i++) { %>
                    <option value="<%= buildingName[i] %>"><%= buildingName[i] %></option> 
                <% } %>
            </select>
        </div>
        <button type="submit">Create</button>
    </form>
    <form action="/backtomanageBS" method="POST">
        <button type="submit">Back</button>    
    </form>
</body>
</html>